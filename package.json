{
  "name": "flow-coding-dojo",
  "version": "0.0.1",
  "main": "server/server.js",
  "scripts": {
    "package": "yarn && yarn --production --ignore-scripts",
    "postdeploy": "echo \"Put here a command to be called during the deployment, like the database migration\"",
    "start": "node .",
    "posttest": "yarn lint && nsp check",
    "client:build": "webpack --config client/webpack/webpack.config.js",
    "client:build:bundle-size": "ANALYSE_BUNDLE=true webpack --config client/webpack/webpack.config.js",
    "client:watch": "webpack-dev-server --config client/webpack/webpack.config.dev.js",
    "client:lint": "eslint --ext .jsx,.js -c client/.eslintrc client/source",
    "client:lint:fix": "eslint --fix --ext .jsx,.js -c client/.eslintrc client/source",
    "client:stylelint": "stylelint client/source/**/*.css",
    "client:test": "NODE_ENV=test jest --maxWorkers=2",
    "client:test:watch": "NODE_ENV=test jest --watch",
    "generate": "plop --plopfile scripts/internals/generators/index.js",
    "lint": "yarn client:lint && yarn server:lint",
    "test": "yarn client:test && yarn server:test:coverage",
    "postinstall": "yarn flow-typed install --overwrite && yarn client:build",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "pretest": "yarn flow && yarn lint && nsp check",
    "server:watch": "nodemon --inspect server/server.js --ignore client/",
    "server:test": "mocha --compilers js:babel-core/register 'server/**/{*,}test.js'",
    "server:test:coverage": "nyc node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- 'server/**/{*,}test.js'",
    "server:lint": "eslint -c server/.eslintrc server"
  },
  "dependencies": {
    "flexboxgrid": "6.3.1",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.0",
    "lodash": "4.17.4",
    "material-ui": "0.19.4",
    "moment": "2.19.3",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-intl": "2.4.0",
    "react-interpolate-component": "0.10.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-test-renderer": "16.0.0",
    "react-tap-event-plugin": "3.0.2",
    "redux": "3.7.2",
    "redux-localstorage": "1.0.0-rc4",
    "redux-localstorage-filter": "0.1.1",
    "redux-thunk": "2.2.0",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "csurf": "1.9.0",
    "debug": "3.1.0",
    "healthcheck-fastit": "git+ssh://git@github.com:fastit/health-check.git#1.0.1",
    "loopback": "3.14.0",
    "loopback-boot": "2.26.2",
    "loopback-component-explorer": "5.1.0",
    "helmet": "3.9.0",
    "serve-favicon": "2.4.5",
    "strong-error-handler": "2.3.0",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "eslint": "4.3.0",
    "babel-eslint": "7.2.3",
    "nsp": "2.6.3",
    "flow-bin": "0.68.0",
    "flow-typed": "2.4.0",
    "babel-preset-flow": "6.23.0",
    "eslint-plugin-flowtype": "2.42.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-istanbul": "0.12.2",
    "babel-jest": "19.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "3.5.0",
    "chromedriver": "2.28.0",
    "clean-webpack-plugin": "0.1.17",
    "css-loader": "0.27.3",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "2.1.2",
    "eslint-plugin-react": "7.1.0",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.30.1",
    "istanbul": "0.4.5",
    "jest-css-modules": "1.1.0",
    "jest": "21.2.1",
    "jsdom-global": "2.1.1",
    "jsdom": "9.12.0",
    "jshint": "2.9.4",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "nock": "9.0.9",
    "nodemon": "1.11.0",
    "nyc": "10.1.2",
    "plop": "1.9.1",
    "postcss-cssnext": "2.10.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "prettier": "1.7.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-mock-store": "1.2.2",
    "sinon": "2.0.0",
    "style-loader": "0.16.1",
    "stylelint-config-standard": "16.0.0",
    "stylelint": "7.10.0",
    "supertest": "3.0.0",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.9.4",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "2.9.0",
    "eslint-config-walmart": "1.2.2",
    "eslint-plugin-filenames": "1.1.0"
  },
  "engines": {
    "node": ">= 6.2"
  },
  "jest": {
    "rootDir": "./client/source",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/CSSStub.js"
    },
    "setupTestFrameworkScriptFile": "../setupTests.js",
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/../../coverage",
    "verbose": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/../../node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  }
}
